import java.util.Scanner; 

public class Main {

	public static void main(String[] args) {
	
	
		String[][] map = {
			{"Mind room", "One", "Two"},
		        {"Four", "Three"}
		
		};
		
		int row = 0;
		int col = 0;
		System.out.println(map[row][col]);
		
		int x;
		
		// Array with predefine ammount of place
		x = 10;
		System.out.println(x);
		int[] numbers = new int [10];
		Scanner input = new.Scanner(System.in);
  		  int a_number = input.nextInt();
		  System.out.println(a_number);
		for (int i = 0; i < numbers.length; i++){
			System.out.prin("Enter a number: ");
			int a_number = input.nextInt();
			numbers[i] = a_number;			 
		}
	
		for(int n : numbers){
			System.out.println(n);
			
		}
	

		   
	} 
}
