package kyh.testing;


public class Main {

    public static void main(String[] args) {


        String[] names = {"Pelle", "Lisa", "Anna", "Fillie"};

        for(String name : names){

            System.out.println(name);

        }

        //String[] map = {"one", "two", "three", "four"};

        String[][] map = {

                {"Mind room", "One", "Two"},

                {"Four", "Three"}
        };


        int row = 0;

        int col = 0;

        System.out.println(map[row][col]);


    //Kontrollera att man har skrivit någt efter go, alltså en riktning
        if (commandPArts.length >= 2){
            //Kolla efter riktning
            if(commandoParts[1].equals.IgnoreCase("north")){
                row--;
                //Kontrollera så vi inte hamnar utanför kartan
                if(row < 0){
                    row = 0;
                    System.out.println("That's impossible, there is a wall"); 
                }
             }
         }
         else if(commandoParts[1].equalsIgnoreCase("south")){
            row++;
            if(row>=map.length){
                row--;
                System.out.println("That's impossible, there is a wall");
            }
         }
         else if(commandParts[1].equalsIgnoreCase("east")){
            col++;
            if(col >= map[row].length){
               col--;
               System.out.println("That's impossible, there is a wall");
            }
         }
         else if(commandParts[1].equalsIgnoreCase("west")){
            col--;
            if(col<0){
                col=0;
                System.out.println("That's impossible, there is a wall");
            }
         }   
         
   



        }


    //Läs kommando från användaren
    
    























    }



